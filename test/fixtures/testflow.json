{
  "workflow": {
    "name": "Testflow",
    "exported": "2017-09-18 02:26:43 UTC",
    "versions": {
      "armagh": "0.5.1",
      "actions": {
        "standard": "0.5.1"
      }
    }
  },
  "actions": [
    {
      "type": "Armagh::StandardActions::NewsmlPublish",
      "action": {
        "name": "testflow-publish"
      },
      "html": {
        "ignore_cdata": "true",
        "force_breaks": "false",
        "unescape_html": "false",
        "preserve_hyperlinks": "false"
      },
      "xml": {
        "html_nodes": "[\"body.content\",\"HeadLine\",\"SubHeadLine\",\"CopyrightLine\"]"
      },
      "input": {
        "docspec": "testflow:ready"
      },
      "output": {
        "docspec": "testflow:published"
      }
    },
    {
      "type": "Armagh::StandardActions::SFTPCollect",
      "action": {
        "name": "testflow-collect"
      },
      "collect": {
        "schedule": "*/10 * * * *",
        "archive": "false",
        "decompress": "false",
        "extract": "false",
        "extract_format": "auto"
      },
      "sftp": {
        "host": "hostname",
        "port": "22",
        "directory_path": "/var/path",
        "duplicate_put_directory_paths": "[]",
        "username": "user",
        "password": "pass",
        "maximum_transfer": "50"
      },
      "input": {
        "docspec": "__COLLECT__testflow-collect:ready"
      },
      "output": {
        "docspec": "testflow:ready"
      }
    },
    {
      "type": "Armagh::StandardActions::TacballConsume",
      "action": {
        "name": "testflow-tacball"
      },
      "tacball": {
        "feed": "testflow",
        "source": "Testflow",
        "type": "TF",
        "attach_orig_file": "false",
        "docid_prefix": "4027",
        "template": "none"
      },
      "sftp": {
        "host": "localhost",
        "port": "22",
        "directory_path": "/var/spool/armagh/dataout/tac",
        "duplicate_put_directory_paths": "[\"/var/spool/armagh/dataout/tacautotest\"]",
        "username": "armagh",
        "maximum_transfer": "50"
      },
      "input": {
        "docspec": "testflow:published"
      }
    }
  ]
}
